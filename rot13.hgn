#! /bin/sh
exec huginn --no-argv -E "${0}" "${@}"
#! huginn

rot13( s ) {
	r = "";
	for ( c : s ) {
		if ( c.is_alpha() ) {
			x = integer( c );
			m = integer( c.is_upper() ? 'A' : 'a' );
			x -= m;
			x += 13;
			x %= 26;
			x += m;
			r += string( character( x ) );
		} else {
			r += string( c );
		}
	}
	return ( r );
}

main() {
	while ( ( line = input() ) != none ) {
		print( rot13( line.strip() ) + "\n" );
	}
}

