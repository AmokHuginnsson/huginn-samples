#! /bin/sh
exec huginn -E "${0}" "${@}"
#! huginn

main( argv_ ) {
	if ( size( argv_ ) > 2 ) {
		print( "script got at least two arguments\n" );
	} else if ( size( argv_ ) > 1 ) {
		print( "script got an argument: " + argv_[1] + "\n" );
	} else {
		print( "script aint got any argument\n" );
	}
	return ( 0 );
}
