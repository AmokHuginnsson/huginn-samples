#! /bin/sh
exec huginn --no-argv -E "${0}"
#! huginn

main() {
	code = "#! /bin/sh{1}exec huginn --no-argv -E {3}${{0}}{3}{1}#! huginn{1}{1}main() {{{1}{2}code = {3}{0}{3};{1}{2}print( code.format( code, {3}{4}n{3}, {3}{4}t{3}, {3}{4}{3}{3}, {3}{4}{4}{3} ) );{1}}}{1}";
	print( code.format( code, "\n", "\t", "\"", "\\" ) );
}
