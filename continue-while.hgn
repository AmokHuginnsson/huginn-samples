#! /bin/sh
exec huginn --no-argv -E "${0}" "${@}"
#! huginn

main() {
	while ( ( line = input() ) != none ) {
		line = line.strip();
		x = integer( line );
		if ( x > 10 ) {
			print( "{} is too much for me :(\n".format( line ) );
			continue;
		}
		print( "{} squared is {}\n".format( line, x * x ) );
	}
	return ( 0 );
}
